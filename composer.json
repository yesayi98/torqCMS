{
    "name": "torq/engine",
    "description": "E-Commerse Content management system",
    "license": "Apache-2.0",
    "version": "2.6.13",
    "require": {
        "php": ">=5.4",
        "phpseclib/phpseclib" : "~2.0",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "roave/security-advisories": "dev-master"
    },
    "archive" : {
        "exclude" : [
            ".*"
        ]
    },
    "autoload" : {
      "files": ["engine/container.php"],
      "psr-0": {
          "Torq": "engine/"
      },

      "classmap": [
          "engine/Core/",
          "engine/Library/",
          "engine/Models/"
      ]
    },
    "config" : {
      "platform": {
        "php": "5.4"
      }
    }
}
